{
  "name": "electron-vue-vite",
  "version": "1.0.0",
  "main": "dist/main/index.cjs",
  "author": "umrjs",
  "license": "MIT",
  "scripts": {
    "dev": "node scripts/watch.mjs",
    "prebuild": "vue-tsc --noEmit --p packages/renderer/tsconfig.json && node scripts/build.mjs",
    "build": "electron-builder",
    "init": "git config core.hooksPath .git/hooks/ && rm -rf .git/hooks && npx simple-git-hooks",
    "test:e2e": "npx playwright test",
    "test:e2e:headless": "npx playwright test --headed"
  },
  "build": {
    "appId": "remoteDesktop",
    "productName": "remoteDesktop",
    "copyright": "Copyright Â© year ${author}",
    "extends": null
  },
  "engines": {
    "node": ">=14.17.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.19.2",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "@vicons/ionicons5": "^0.12.0",
    "@vitejs/plugin-vue": "^2.1.0",
    "electron": "16.0.8",
    "electron-builder": "^22.14.5",
    "electron-rebuild": "^3.2.7",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.5.0",
    "naive-ui": "^2.26.4",
    "nano-staged": "^0.6.0",
    "node-sass": "^7.0.1",
    "sass": "^1.49.9",
    "simple-git-hooks": "^2.7.0",
    "typescript": "^4.5.5",
    "vfonts": "^0.0.3",
    "vite": "^2.7.13",
    "vite-plugin-resolve": "^1.8.0",
    "vue": "^3.2.29",
    "vue-router": "^4.0.14",
    "vue-tsc": "^0.31.1"
  },
  "env": {
    "VITE_DEV_SERVER_HOST": "127.0.0.1",
    "VITE_DEV_SERVER_PORT": 3344
  },
  "keywords": [
    "vite",
    "electron",
    "vue3",
    "rollup"
  ],
  "dependencies": {
    "@types/axios": "^0.14.0",
    "@types/lodash": "^4.14.182",
    "@types/ws": "^8.5.3",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "@vueuse/core": "^8.2.6",
    "axios": "^0.26.1",
    "eslint-config-airbnb": "^19.0.4",
    "lodash": "^4.17.21",
    "pinia": "^2.0.13",
    "robotjs": "^0.6.0",
    "vkey": "^1.0.1",
    "ws": "^8.5.0"
  }
}
